{
  "name": "backend",
  "version": "0.0.0",
  "main": "index.js",
  "repository": "https://github.com/larafe1/ex3.git",
  "author": "larafe1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node -r ts-node/register -r tsconfig-paths/register dist/server.js",
    "dev": "tsnd --respawn --transpile-only --exit-child -r tsconfig-paths/register src/server.ts",
    "build": "tsc --project tsconfig.json&&tscpaths -p tsconfig.json -s src -o dist",
    "lint": "eslint src --ext ts --max-warnings=0",
    "lint:fix": "eslint src --ext ts --max-warnings=0 --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --check \"src/**/*.ts\" --write",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "@prisma/client": "^4.12.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "graphql": "^16.6.0",
    "graphql-relay": "^0.10.0",
    "jsonwebtoken": "^9.0.0",
    "koa": "^2.14.2",
    "koa-graphql": "^0.12.0",
    "prisma": "^4.12.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/koa": "^2.13.6",
    "@types/koa__cors": "^4.0.0",
    "@types/koa__router": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
